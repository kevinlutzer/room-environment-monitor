{
  "name": "personal-website-gcf",
  "version": "0.0.0",
  "private": true,
  "author": "Kevin Lutzer",
  "main" : "build/main.js",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "build": "rm -rf build && tsc",
    "deploy": "run(){ gcloud beta functions deploy RaspberryPiRoomMonitoringPubSubHandler --trigger-topic raspberry-pi-environment-monitoring-topic; }; tsc && run"
  },
  "dependencies": {
    "@google-cloud/datastore": "1.1.0",
    "@google-cloud/functions-emulator": "^1.0.0-alpha.25",
    "@types/express": "^4.0.35",
    "@types/moment": "^2.13.0",
    "@types/node": "^7.0.8",
    "typescript": "^2.2.2"
  },
  "devDependencies": {
    "@google-cloud/nodejs-repo-tools": "1.4.17"
  },
  "cloud-repo-tools": {
    "requiresKeyFile": true,
    "requiresProjectId": true
  }
}